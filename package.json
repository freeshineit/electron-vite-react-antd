{
  "name": "electron-vite-react",
  "version": "0.1.0",
  "main": "dist-electron/main/index.js",
  "description": "Electron Vite React boilerplate.",
  "author": "ShineShao <xiaoshaoqq@gmail.com>",
  "license": "MIT",
  "private": true,
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:7777/"
    }
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && electron-builder",
    "preview": "vite preview",
    "preinstall": "npx only-allow pnpm",
    "pree2e": "vite build --mode=test",
    "e2e": "playwright test",
    "fix": "eslint --fix src/**/*.ts --fix  src/**/*.tsx",
    "lint": "eslint --ext .ts,tsx,.js,.jsx ./src",
    "fmt": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,scss,cjs}\""
  },
  "dependencies": {
    "electron-updater": "^6.1.8"
  },
  "devDependencies": {
    "@ant-design/icons": "^5.3.6",
    "@reduxjs/toolkit": "^2.2.3",
    "@playwright/test": "^1.44.0",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.2.1",
    "antd": "^5.17.2",
    "autoprefixer": "^10.4.19",
    "axios": "^1.6.8",
    "classnames": "^2.5.1",
    "cross-env": "^7.0.3",
    "electron": "^29",
    "electron-builder": "^24.13.3",
    "eslint": "^8.57.0",
    "eslint-config-xx": "^1.10.1",
    "immer": "^10.0.4",
    "lowdb": "^7.0.1",
    "only-allow": "^1.2.1",
    "postcss": "^8.4.38",
    "postcss-import": "^16.1.0",
    "prettier": "^3.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.1",
    "react-redux": "^9.1.1",
    "redux": "^5.0.1",
    "sass": "^1.77.1",
    "tailwindcss": "^3.4.1",
    "terser": "^5.31.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vite-plugin-electron": "^0.28.7",
    "vite-plugin-electron-renderer": "^0.14.5"
  }
}
